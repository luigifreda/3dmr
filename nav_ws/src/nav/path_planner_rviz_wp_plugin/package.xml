<?xml version="1.0"?>
<package>
  <name>path_planner_rviz_wp_plugin</name>
  <version>0.0.0</version>
  <description>The path_planner_rviz_wp_plugin package </description>

  <!-- One maintainer tag required, multiple allowed, one person per tag --> 
  <!-- Example:  -->
  <!-- <maintainer email="jane.doe@example.com">Jane Doe</maintainer> -->
  <maintainer email="freda@diag.uniroma1.it">Luigi Freda</maintainer>


  <!-- One license tag required, multiple allowed, one license per tag -->
  <!-- Commonly used license strings: -->
  <!--   BSD, MIT, Boost Software License, GPLv2, GPLv3, LGPLv2.1, LGPLv3 -->
  <license>GPLv3</license>


  <!-- Url tags are optional, but mutiple are allowed, one per tag -->
  <!-- Optional attribute type can be: website, bugtracker, or repository -->
  <!-- Example: -->
  <!-- <url type="website">http://wiki.ros.org/rviz_waypoint_plugin</url> -->


  <!-- Author tags are optional, mutiple are allowed, one per tag -->
  <!-- Authors do not have to be maintianers, but could be -->
  <!-- Example: -->
  <!-- <author email="jane.doe@example.com">Jane Doe</author> -->


  <!-- The *_depend tags are used to specify dependencies -->
  <!-- Dependencies can be catkin packages or system dependencies -->
  <!-- Examples: -->
  <!-- Use build_depend for packages you need at compile time: -->
  <!--   <build_depend>message_generation</build_depend> -->
  <!-- Use buildtool_depend for build tool packages: -->
  <!--   <buildtool_depend>catkin</buildtool_depend> -->
  <!-- Use run_depend for packages you need at runtime: -->
  <!--   <run_depend>message_runtime</run_depend> -->
  <!-- Use test_depend for packages you need only for testing: -->
  <!--   <test_depend>gtest</test_depend> -->
  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>class_loader</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>nav_msgs</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>message_filters</build_depend>
  <build_depend>trajectory_control_msgs</build_depend>
  <build_depend>patrolling_build_graph_msgs</build_depend>
  <build_depend>path_planner</build_depend>
  <build_depend>rviz</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>exploration_msgs</build_depend> 
  <build_depend>wireless_network_msgs</build_depend>   
  
  <run_depend>class_loader</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>nav_msgs</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>message_filters</run_depend>
  <run_depend>trajectory_control_msgs</run_depend>
  <run_depend>patrolling_build_graph_msgs</run_depend>  
  <run_depend>path_planner</run_depend>
  <run_depend>rviz</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>dynamic_reconfigure</run_depend>  
  <run_depend>exploration_msgs</run_depend> 
  <run_depend>wireless_network_msgs</run_depend>   
     

  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <!-- Other tools can request additional information be placed here -->
    <rviz plugin="${prefix}/plugin_description.xml"/>
    <rviz plugin="${prefix}/plugin_description_SIM1.xml"/>
    <rviz plugin="${prefix}/plugin_description_SIM2.xml"/>
    <rviz plugin="${prefix}/plugin_description_SIM3.xml"/>
    <rviz plugin="${prefix}/plugin_description_SIM_JACKAL1.xml"/>
    <rviz plugin="${prefix}/plugin_description_SIM_JACKAL2.xml"/>    
  </export>
  

</package>
